    <center>
        <div class="box">
            <div class="bill-selection">
                <h3>Select a Bill to Pay</h3><br>

                <label for="electricity">
                    <img src="data:image/png;base64," alt="Electricity">
                    Electricity Bill
                    <input type="radio" name="bill" id="electricity" value="electricity" required>
                </label>
                <label for="water">
                    <img src="data:image/png;base64," alt="Water">
                    Water Bill
                    <input type="radio" name="bill" id="water" value="water">
                </label>
                <label for="internet">
                    <img src="data:image/png;base64," alt="Internet">
                    Internet Bill
                    <input type="radio" name="bill" id="internet" value="internet">

                </label><br><br>

            </div>


            <div class="payment-methods">
                <h3>Choose Your Payment Method</h3><br>
                <div class="payment-method">
                    <a href="Visa.jsp">
                        <img src="images/Visa.png">
                    </a>

                    <center>Credit/Debit Card</center>
                </div>

                <div class="payment-method">
                    <!-- Custom PayPal button -->
                    <a href="Paypal.jsp">
                        <img src="images/Paypal.png" alt="Pay with PayPal">
                    </a>
                    <center>E-Banking</center>
                </div>


                <!-- Hidden div for the actual PayPal button -->
                <div id="paypal-button-container" style="display:none;"></div>
            </div>
        </div>
